{
  "name": "woocommerce-sdk",
  "version": "0.1.0",
  "description": "Complete TypeScript SDK for WooCommerce REST API v3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "woocommerce",
    "wordpress",
    "api",
    "sdk",
    "typescript",
    "e-commerce",
    "rest-api"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "crypto-js": "^4.2.0",
    "oauth-1.0a": "^2.2.6"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^2.1.8",
    "typescript": "^5.3.3",
    "vitest": "^2.1.8"
  }
}